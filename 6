Lista 01 — Laço de Repetição FOR
Exercício 1:

javascript
Copiar código
// Exercício 1 - Laço FOR
let primeiroNumero = parseInt(prompt("Digite o primeiro número do intervalo:"));
let ultimoNumero = parseInt(prompt("Digite o último número do intervalo:"));

if (primeiroNumero < ultimoNumero) {
    console.log(`No intervalo entre ${primeiroNumero} e ${ultimoNumero}:`);
    for (let i = primeiroNumero; i <= ultimoNumero; i++) {
        if (i % 3 === 0 && i % 5 === 0) {
            console.log(`${i} é múltiplo de 3 e 5`);
        }
    }
} else {
    console.log("Intervalo inválido!");
}
Exercício 2:

javascript
Copiar código
// Exercício 2 - Contagem de números pares e ímpares
let pares = 0;
let impares = 0;

for (let i = 1; i <= 10; i++) {
    let numero = parseInt(prompt(`Digite o ${i}º número:`));
    if (numero % 2 === 0) {
        pares++;
    } else {
        impares++;
    }
}

console.log(`Total de números pares: ${pares}`);
console.log(`Total de números ímpares: ${impares}`);
Lista 02 — Laço de Repetição WHILE
Exercício 1:

javascript
Copiar código
// Exercício 1 - Contagem de idades
let menoresDe21 = 0;
let maioresDe50 = 0;
let idade;

while (true) {
    idade = parseInt(prompt("Digite uma idade:"));
    if (idade < 0) {
        break;
    }
    if (idade < 21) {
        menoresDe21++;
    } else if (idade > 50) {
        maioresDe50++;
    }
}

console.log(`Total de pessoas menores de 21 anos: ${menoresDe21}`);
console.log(`Total de pessoas maiores de 50 anos: ${maioresDe50}`);
Exercício 2:

javascript
Copiar código
// Exercício 2 - Pesquisa de colaboradores
let totalBackend = 0;
let totalMulheresFrontend = 0;
let totalHomensMobile40 = 0;
let totalNaoBinariosFullStack30 = 0;
let totalPessoas = 0;
let somaIdades = 0;

while (true) {
    let idade = parseInt(prompt("Idade:"));
    if (idade < 0) break;
    let genero = parseInt(prompt("Identidade de Gênero (1-6):"));
    let desenvolvedor = parseInt(prompt("Pessoa Desenvolvedora (1-4):"));

    totalPessoas++;
    somaIdades += idade;

    if (desenvolvedor === 1) totalBackend++;
    if ((genero === 1 || genero === 4) && desenvolvedor === 2) totalMulheresFrontend++;
    if ((genero === 2 || genero === 5) && desenvolvedor === 3 && idade > 40) totalHomensMobile40++;
    if (genero === 3 && desenvolvedor === 4 && idade < 30) totalNaoBinariosFullStack30++;
}

console.log(`Total de pessoas desenvolvedoras Backend: ${totalBackend}`);
console.log(`Total de Mulheres Cis e Trans desenvolvedoras Frontend: ${totalMulheresFrontend}`);
console.log(`Total de Homens Cis e Trans desenvolvedores Mobile maiores de 40 anos: ${totalHomensMobile40}`);
console.log(`Total de Pessoas Não Binárias desenvolvedoras FullStack menores de 30 anos: ${totalNaoBinariosFullStack30}`);
console.log(`O número total de pessoas que responderam à pesquisa: ${totalPessoas}`);
console.log(`A média de idade das pessoas que responderam à pesquisa: ${(somaIdades / totalPessoas).toFixed(2)}`);
Lista 03 — Laço de Repetição DO…WHILE
Exercício 1:

javascript
Copiar código
// Exercício 1 - Soma de números positivos
let somaPositivos = 0;
let numero;

do {
    numero = parseInt(prompt("Digite um número:"));
    if (numero > 0) {
        somaPositivos += numero;
    }
} while (numero !== 0);

console.log(`A soma dos números positivos é: ${somaPositivos}`);
Exercício 2:

javascript
Copiar código
// Exercício 2 - Média de múltiplos de 3
let somaMultiplosDe3 = 0;
let countMultiplosDe3 = 0;
let numero;

do {
    numero = parseInt(prompt("Digite um número:"));
    if (numero % 3 === 0) {
        somaMultiplosDe3 += numero;
        countMultiplosDe3++;
    }
} while (numero !== 0);

if (countMultiplosDe3 > 0) {
    console.log(`A média de todos os números múltiplos de 3 é: ${(somaMultiplosDe3 / countMultiplosDe3).toFixed(2)}`);
} else {
    console.log("Não foram digitados múltiplos de 3.");
}
Lista 04 — Vetores
Exercício 1:

javascript
Copiar código
// Exercício 1 - Pesquisa no vetor
let vetor = [2, 5, 1, 3, 4, 9, 7, 8, 10, 6];
let numeroProcurado = parseInt(prompt("Digite o número que você deseja encontrar:"));
let index = vetor.indexOf(numeroProcurado);

if (index !== -1) {
    console.log(`O número ${numeroProcurado} está localizado na posição: ${index}`);
} else {
    console.log(`O número ${numeroProcurado} não foi encontrado!`);
}
Exercício 2:

javascript
Copiar código
// Exercício 2 - Operações com o vetor
let vetor = [2, 5, 1, 3, 4, 9, 7, 8, 10, 6];
let soma = 0;

console.log("Elementos nos índices ímpares:");
for (let i = 1; i < vetor.length; i += 2) {
    console.log(vetor[i]);
}

console.log("Elementos pares:");
for (let i = 0; i < vetor.length; i++) {
    if (vetor[i] % 2 === 0) {
        console.log(vetor[i]);
    }
}

vetor.forEach(num => soma += num);

let media = soma / vetor.length;
console.log(`Soma: ${soma}`);
console.log(`Média: ${media.toFixed(2)}`);
Lista 05 — Matrizes
Exercício 1:

javascript
Copiar código
// Exercício 1 - Matrizes 3x3
let matriz = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
];

let somaDiagonalPrincipal = 0;
let somaDiagonalSecundaria = 0;

console.log("Elementos da Diagonal Principal:");
for (let i = 0; i < 3; i++) {
    somaDiagonalPrincipal += matriz[i][i];
    console.log(matriz[i][i]);
}

console.log("Elementos da Diagonal Secundária:");
for (let i = 0; i < 3; i++) {
    somaDiagonalSecundaria += matriz[i][2 - i];
    console.log(matriz[i][2 - i]);
}

console.log(`Soma dos Elementos da Diagonal Principal: ${somaDiagonalPrincipal}`);
console.log(`Soma dos Elementos da Diagonal Secundária: ${somaDiagonalSecundaria}`);
Exercício 2:

javascript
Copiar código
// Exercício 2 - Matrizes e médias
let matriz = [
    [4.0, 5.0, 7.0, 3.0],
    [2.5, 6.5, 4.7, 8.0],
    [4.8, 5.4, 9.0, 7.8],
    [5.3, 8.1, 3.1, 9.1],
    [6.2, 8.9, 10.0, 8.0],
    [9.0, 6.5, 7.6, 8.2],
    [5.0, 5.0, 5.0, 6.3],
    [7.0, 8.0, 9.0, 8.5],
    [5.5, 3.5, 2.5, 1.0],
    [8.0, 9.0, 10.0, 9.5]
];

let medias = [];

for (let i = 0; i < matriz.length; i++) {
    let somaNotas = 0;
    for (let j = 0; j < matriz[i].length; j++) {
        somaNotas += matriz[i][j];
    }
    medias.push(somaNotas / matriz[i].length);
}

medias.forEach((media, i) => console.log(`Média do participante ${i+1}: ${media.toFixed(2)}`));
